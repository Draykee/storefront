{
  "name": "vendure-storefront",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lib:build": "ng build storefront",
    "lib:watch": "ng build storefront --watch",
    "start": "ng serve --port 4201",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-types": "gql-gen --config codegen/codegen.yml",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run vendure-storefront:server:production"
  },
  "private": true,
  "dependencies": {
