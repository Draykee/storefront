{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "../out-tsc/app-server",
    "baseUrl": ".",
    "paths": {
      "@vendure/storefront": [
        "../dist/storefront"
      ],
      "*": [
        "../projects/storefront/node_modules/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "entryModule": "app/app.server.module#AppServerModule"
  }
,
  "files": [
    "main.server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
